<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
<class name="com.jwt.hibernate.bean.Company" table="cep_simulator">
<id name="Date" column="Date">
<generator class="assigned"/>
</id>

<property name="PERMNO" column="PERMNO"/>
<property name="PRC" column="PRC"/>
<property name="Pseudo_PRC" column="Pseudo_PRC"/>
<property name="Turnover" column="Turnover"/>

</class>
</hibernate-mapping>